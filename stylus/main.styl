// reset

*
	border 0
	margin 0
	padding 0

// styles

body
	background #eee
	font 14px Helvetica, Arial, sans-serif
	padding 20px

li
	display inline
	list-style none
	margin 0 -3px 0 0

	a
		background #999
		display inline-block
		margin 10px 0 0 10px

		&:hover
			background #888
		
		img
			display block
			width 200px